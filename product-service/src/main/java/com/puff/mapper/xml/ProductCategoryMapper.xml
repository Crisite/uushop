<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.puff.mapper.ProductCategoryMapper">
    <select id="findNameByType" resultType="string">
        select uushop.product_category.category_name from uushop.product_category where category_type = #{type}
    </select>
</mapper>
